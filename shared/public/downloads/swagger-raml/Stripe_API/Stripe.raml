                              "used": false,
                              "object": "token",
                              "type": "card",
                              "card": {
                                "id": "cc_1RjCGyzfsxF0AF",
                                "object": "card",
                                "last4": "4242",
                                "type": "Visa",
                                "exp_month": 8,
                                "exp_year": 2014,
                                "fingerprint": "qhjxpr7DiCdFYTlH",
                                "customer": null,
                                "country": "US",
                                "name": null,
