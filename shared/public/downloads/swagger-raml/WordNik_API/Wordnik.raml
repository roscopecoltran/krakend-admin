        AuthenticationToken: '{"simple":false,"type":"object","properties":{"userSignature":{"required":false,"type":"string"},"userId":{"format":"int64","required":false,"type":"integer"},"token":{"required":false,"type":"string"}}}'
        Category: '{"simple":false,"required":["id"],"type":"object","properties":{"name":{"required":false,"type":"string"},"id":{"format":"int64","required":false,"type":"integer"}}}'
        User: '{"simple":false,"type":"object","properties":{"password":{"required":false,"type":"string"},"displayName":{"required":false,"type":"string"},"faceBookId":{"required":false,"type":"string"},"id":{"format":"int64","required":false,"type":"integer"},"userName":{"required":false,"type":"string"},"email":{"required":false,"type":"string"},"status":{"format":"int32","required":false,"type":"integer"},"username":{"required":false,"type":"string"}}}'
        ContentProvider: '{"simple":false,"type":"object","properties":{"name":{"required":false,"type":"string"},"id":{"format":"int32","required":false,"type":"integer"}}}'
        FrequencySummary: |-
