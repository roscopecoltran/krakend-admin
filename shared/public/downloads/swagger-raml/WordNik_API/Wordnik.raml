/wordLists.json: 
    post: 
        headers: 
            auth_token: 
                description: The auth token of the logged-in user, obtained by calling /account.{format}/authenticate/{username} (described above)
                type: string
                required: true
                repeat: false
        responses: 
            "200": 
                description: No response was specified
/account.json/apiTokenStatus: 
    get: 
        headers: 
            api_key: 
                description: Wordnik authentication token
                type: string
                required: false
                repeat: false
        responses: 
            "200": 
                description: No response was specified
/account.json/authenticate/{username}: 
    get: 
        queryParameters: 
            password: 
                description: The user's password
                type: string
                required: true
                repeat: false
        responses: 
            "200": 
                description: No response was specified
        baseUriParameters: 
            username: 
                description: A confirmed Wordnik username
                type: string
                required: true
                repeat: false
    post: 
        responses: 
            "200": 
                description: No response was specified
        baseUriParameters: 
            username: 
                description: A confirmed Wordnik username
                type: string
                required: true
                repeat: false
/word.json/{word}: 
    get: 
        queryParameters: 
            useCanonical: 
                description: If true will try to return the correct word root ('cats' -> 'cat'). If false returns exactly what was requested.
                type: string
                required: false
                repeat: false
                default: "false"
            includeSuggestions: 
                description: Return suggestions (for correct spelling, case variants, etc.)
                type: string
                required: false
                repeat: false
                default: "true"
        responses: 
            "200": 
                description: No response was specified
        baseUriParameters: 
            word: 
                description: String value of WordObject to return
                type: string
                required: true
                repeat: false
    /pronunciations: 
        get: 
            queryParameters: 
                useCanonical: 
                    description: If true will try to return a correct word root ('cats' -> 'cat'). If false returns exactly what was requested.
                    type: string
                    required: false
                    repeat: false
                    default: "false"
                sourceDictionary: 
                    description: Get from a single dictionary
                    type: string
                    required: false
                    repeat: false
                limit: 
                    description: Maximum number of results to return
                    type: integer
                    required: false
                    repeat: false
                    default: "50"
                typeFormat: 
                    description: Text pronunciation type
                    type: string
                    required: false
                    repeat: false
            responses: 
                "200": 
                    description: No response was specified
            baseUriParameters: 
                word: 
                    description: Word to get pronunciations for
                    type: string
                    required: true
                    repeat: false
    /frequency: 
        get: 
            queryParameters: 
                useCanonical: 
                    description: If true will try to return the correct word root ('cats' -> 'cat'). If false returns exactly what was requested.
                    type: string
                    required: false
                    repeat: false
                    default: "false"
                startYear: 
                    description: Starting Year
                    type: integer
                    required: false
                    repeat: false
                    default: "1800"
                endYear: 
                    description: Ending Year
                    type: integer
                    required: false
                    repeat: false
                    default: "2012"
            responses: 
                "200": 
                    description: No response was specified
            baseUriParameters: 
                word: 
                    description: Word to return
                    type: string
                    required: true
                    repeat: false
    /phrases: 
        get: 
            queryParameters: 
                useCanonical: 
                    description: If true will try to return the correct word root ('cats' -> 'cat'). If false returns exactly what was requested.
                    type: string
                    required: false
                    repeat: false
                    default: "false"
                wlmi: 
                    description: Minimum WLMI for the phrase
                    type: integer
                    required: false
                    repeat: false
                    default: "0"
                limit: 
                    description: Maximum number of results to return
                    type: integer
                    required: false
                    repeat: false
                    default: "5"
            responses: 
                "200": 
                    description: No response was specified
            baseUriParameters: 
                word: 
                    description: Word to fetch phrases for
                    type: string
                    required: true
                    repeat: false
    /etymologies: 
        get: 
            queryParameters: 
                useCanonical: 
                    description: If true will try to return the correct word root ('cats' -> 'cat'). If false returns exactly what was requested.
                    type: string
                    required: false
                    repeat: false
            responses: 
                "200": 
                    description: No response was specified
            baseUriParameters: 
                word: 
                    description: Word to return
                    type: string
                    required: true
                    repeat: false
    /relatedWords: 
        get: 
            queryParameters: 
                useCanonical: 
                    description: If true will try to return the correct word root ('cats' -> 'cat'). If false returns exactly what was requested.
                    type: string
                    required: false
                    repeat: false
                    default: "false"
                limitPerRelationshipType: 
                    description: Restrict to the supplied relationship types
                    type: integer
                    required: false
                    repeat: false
                    default: "10"
                relationshipTypes: 
                    description: Limits the total results per type of relationship type
                    type: string
                    required: false
                    repeat: false
            responses: 
                "200": 
                    description: No response was specified
            baseUriParameters: 
                word: 
                    description: Word to fetch relationships for
                    type: string
                    required: true
                    repeat: false
    /audio: 
        get: 
            description: The metadata includes a time-expiring fileUrl which allows reading the audio file directly from the API.  Currently only audio pronunciations from the American Heritage Dictionary in mp3 format are supported.
            queryParameters: 
                useCanonical: 
                    description: Use the canonical form of the word
                    type: string
                    required: false
                    repeat: false
                    default: "false"
                limit: 
                    description: Maximum number of results to return
                    type: integer
                    required: false
                    repeat: false
                    default: "50"
            responses: 
                "200": 
                    description: No response was specified
            baseUriParameters: 
                word: 
                    description: Word to get audio for.
                    type: string
                    required: true
                    repeat: false
    /examples: 
        get: 
            queryParameters: 
                includeDuplicates: 
                    description: Show duplicate examples from different sources
                    type: string
                    required: false
                    repeat: false
                    default: "false"
                useCanonical: 
                    description: If true will try to return the correct word root ('cats' -> 'cat'). If false returns exactly what was requested.
                    type: string
                    required: false
                    repeat: false
                    default: "false"
                limit: 
                    description: Maximum number of results to return
                    type: integer
                    required: false
                    repeat: false
                    default: "5"
                skip: 
                    description: Results to skip
                    type: integer
                    required: false
                    repeat: false
                    default: "0"
            responses: 
                "200": 
                    description: No response was specified
            baseUriParameters: 
                word: 
                    description: Word to return examples for
                    type: string
                    required: true
                    repeat: false
    /definitions: 
        get: 
            queryParameters: 
                sourceDictionaries: 
                    description: "Source dictionary to return definitions from.  If 'all' is received, results are returned from all sources. If multiple values are received (e.g. 'century,wiktionary'), results are returned from the first specified dictionary that has definitions. If left blank, results are returned from the first dictionary that has definitions. By default, dictionaries are searched in this order: ahd, wiktionary, webster, century, wordnet"
                    type: string
                    required: false
                    repeat: true
                useCanonical: 
                    description: If true will try to return the correct word root ('cats' -> 'cat'). If false returns exactly what was requested.
                    type: string
                    required: false
                    repeat: false
                    default: "false"
                partOfSpeech: 
                    description: CSV list of part-of-speech types
                    type: string
                    required: false
                    repeat: false
                includeTags: 
                    description: Return a closed set of XML tags in response
                    type: string
                    required: false
                    repeat: false
                    default: "false"
                limit: 
                    description: Maximum number of results to return
                    type: integer
                    required: false
                    repeat: false
                    default: "200"
                includeRelated: 
                    description: Return related words with definitions
                    type: string
                    required: false
                    repeat: false
                    default: "false"
