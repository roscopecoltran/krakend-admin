        Label: '{"simple":false,"type":"object","properties":{"text":{"required":false,"type":"string"},"type":{"required":false,"type":"string"}}}'
        TextPron: '{"simple":false,"type":"object","properties":{"rawType":{"required":false,"type":"string"},"raw":{"required":false,"type":"string"},"seq":{"format":"int32","required":false,"type":"integer"}}}'
        FacetValue: '{"simple":false,"type":"object","properties":{"count":{"format":"int64","required":false,"type":"integer"},"value":{"required":false,"type":"string"}}}'
        SimpleExample: '{"simple":false,"type":"object","properties":{"id":{"format":"int64","required":false,"type":"integer"},"text":{"required":false,"type":"string"},"title":{"required":false,"type":"string"},"url":{"required":false,"type":"string"}}}'
        Example: |-
                {"simple":false,"type":"object","properties":{"sentence":{"required":false,"type":"ref","simpleRef":"Sentence","refFormat":"INTERNAL","$ref":"#\/definitions\/Sentence"},"score":{"required":false,"type":"ref","simpleRef":"ScoredWord","refFormat":"INTERNAL","$ref":"#\/definitions\/ScoredWord"},"exampleId":{"format":"int64","required":false,"type":"integer"},"provider":{"required":false,"type":"ref","simpleRef":"ContentProvider","refFormat":"INTERNAL","$ref":"#\/definitions\/ContentProvider"},"year":{"format":"int32","required":false,"type":"integer"},"rating":{"format":"float","required":false,"type":"number"},"documentId":{"format":"int64","required":false,"type":"integer"},"id":{"format":"int64","required":false,"type":"integer"},"text":{"required":false,"type":"string"},"title":{"required":false,"type":"string"},"word":{"required":false,"type":"string"},"url":{"required":false,"type":"string"}}}
