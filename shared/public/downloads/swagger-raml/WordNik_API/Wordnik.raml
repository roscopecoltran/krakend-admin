        ExampleUsage: '{"simple":false,"type":"object","properties":{"text":{"required":false,"type":"string"}}}'
        PartOfSpeech: |-
                {"simple":false,"type":"object","properties":{"allCategories":{"required":false,"type":"array","items":{"required":false,"type":"ref","simpleRef":"Category","refFormat":"INTERNAL","$ref":"#\/definitions\/Category"}},"roots":{"required":false,"type":"array","items":{"required":false,"type":"ref","simpleRef":"Root","refFormat":"INTERNAL","$ref":"#\/definitions\/Root"}},"storageAbbr":{"required":false,"type":"array","items":{"required":false,"type":"string"}}}}
        Citation: '{"simple":false,"type":"object","properties":{"cite":{"required":false,"type":"string"},"source":{"required":false,"type":"string"}}}'
        WordListWord: '{"simple":false,"required":["id"],"type":"object","properties":{"createdAt":{"format":"date","required":false,"type":"string"},"numberLists":{"format":"int64","required":false,"type":"integer"},"id":{"format":"int64","required":false,"type":"integer"},"numberCommentsOnWord":{"format":"int64","required":false,"type":"integer"},"userId":{"format":"int64","required":false,"type":"integer"},"word":{"required":false,"type":"string"},"username":{"required":false,"type":"string"}}}'
        StringValue: '{"simple":false,"type":"object","properties":{"word":{"required":false,"type":"string"}}}'
        WordOfTheDay: |-
                {"simple":false,"required":["id"],"type":"object","properties":{"contentProvider":{"required":false,"type":"ref","simpleRef":"ContentProvider","refFormat":"INTERNAL","$ref":"#\/definitions\/ContentProvider"},"createdAt":{"format":"date","required":false,"type":"string"},"note":{"required":false,"type":"string"},"examples":{"required":false,"type":"array","items":{"required":false,"type":"ref","simpleRef":"SimpleExample","refFormat":"INTERNAL","$ref":"#\/definitions\/SimpleExample"}},"createdBy":{"required":false,"type":"string"},"publishDate":{"format":"date","required":false,"type":"string"},"htmlExtra":{"required":false,"type":"string"},"id":{"format":"int64","required":false,"type":"integer"},"category":{"required":false,"type":"string"},"definitions":{"required":false,"type":"array","items":{"required":false,"type":"ref","simpleRef":"SimpleDefinition","refFormat":"INTERNAL","$ref":"#\/definitions\/SimpleDefinition"}},"word":{"required":false,"type":"string"},"parentId":{"required":false,"type":"string"}}}
        Sentence: |-
                {"simple":false,"type":"object","properties":{"documentMetadataId":{"format":"int64","required":false,"type":"integer"},"display":{"required":false,"type":"string"},"rating":{"format":"int32","required":false,"type":"integer"},"scoredWords":{"required":false,"type":"array","items":{"required":false,"type":"ref","simpleRef":"ScoredWord","refFormat":"INTERNAL","$ref":"#\/definitions\/ScoredWord"}},"id":{"format":"int64","required":false,"type":"integer"},"hasScoredWords":{"required":false,"type":"boolean"}}}
        Syllable: '{"simple":false,"type":"object","properties":{"text":{"required":false,"type":"string"},"type":{"required":false,"type":"string"},"seq":{"format":"int32","required":false,"type":"integer"}}}'
        Facet: |-
                {"simple":false,"type":"object","properties":{"facetValues":{"required":false,"type":"array","items":{"required":false,"type":"ref","simpleRef":"FacetValue","refFormat":"INTERNAL","$ref":"#\/definitions\/FacetValue"}},"name":{"required":false,"type":"string"}}}
        Root: |-
                {"simple":false,"required":["id"],"type":"object","properties":{"name":{"required":false,"type":"string"},"categories":{"required":false,"type":"array","items":{"required":false,"type":"ref","simpleRef":"Category","refFormat":"INTERNAL","$ref":"#\/definitions\/Category"}},"id":{"format":"int64","required":false,"type":"integer"}}}
        Bigram: '{"simple":false,"type":"object","properties":{"wlmi":{"format":"double","required":false,"type":"number"},"count":{"format":"int64","required":false,"type":"integer"},"gram1":{"required":false,"type":"string"},"gram2":{"required":false,"type":"string"},"mi":{"format":"double","required":false,"type":"number"}}}'
        Definition: |-
                {"simple":false,"type":"object","properties":{"notes":{"required":false,"type":"array","items":{"required":false,"type":"ref","simpleRef":"Note","refFormat":"INTERNAL","$ref":"#\/definitions\/Note"}},"exampleUses":{"required":false,"type":"array","items":{"required":false,"type":"ref","simpleRef":"ExampleUsage","refFormat":"INTERNAL","$ref":"#\/definitions\/ExampleUsage"}},"extendedText":{"required":false,"type":"string"},"partOfSpeech":{"required":false,"type":"string"},"attributionText":{"required":false,"type":"string"},"labels":{"required":false,"type":"array","items":{"required":false,"type":"ref","simpleRef":"Label","refFormat":"INTERNAL","$ref":"#\/definitions\/Label"}},"score":{"format":"float","required":false,"type":"number"},"sequence":{"required":false,"type":"string"},"citations":{"required":false,"type":"array","items":{"required":false,"type":"ref","simpleRef":"Citation","refFormat":"INTERNAL","$ref":"#\/definitions\/Citation"}},"sourceDictionary":{"required":false,"type":"string"},"textProns":{"required":false,"type":"array","items":{"required":false,"type":"ref","simpleRef":"TextPron","refFormat":"INTERNAL","$ref":"#\/definitions\/TextPron"}},"attributionUrl":{"required":false,"type":"string"},"text":{"required":false,"type":"string"},"word":{"required":false,"type":"string"},"relatedWords":{"required":false,"type":"array","items":{"required":false,"type":"ref","simpleRef":"Related","refFormat":"INTERNAL","$ref":"#\/definitions\/Related"}},"seqString":{"required":false,"type":"string"}}}
        ScoredWord: '{"simple":false,"type":"object","properties":{"docTermCount":{"format":"int32","required":false,"type":"integer"},"score":{"format":"float","required":false,"type":"number"},"wordType":{"required":false,"type":"string"},"partOfSpeech":{"required":false,"type":"string"},"lemma":{"required":false,"type":"string"},"sentenceId":{"format":"int64","required":false,"type":"integer"},"stopword":{"required":false,"type":"boolean"},"baseWordScore":{"format":"double","required":false,"type":"number"},"id":{"format":"int64","required":false,"type":"integer"},"position":{"format":"int32","required":false,"type":"integer"},"word":{"required":false,"type":"string"}}}'
        WordList: '{"simple":false,"type":"object","properties":{"createdAt":{"format":"date","required":false,"type":"string"},"name":{"required":false,"type":"string"},"description":{"required":false,"type":"string"},"lastActivityAt":{"format":"date","required":false,"type":"string"},"id":{"format":"int64","required":false,"type":"integer"},"numberWordsInList":{"format":"int64","required":false,"type":"integer"},"permalink":{"required":false,"type":"string"},"type":{"required":false,"type":"string"},"userId":{"format":"int64","required":false,"type":"integer"},"updatedAt":{"format":"date","required":false,"type":"string"},"username":{"required":false,"type":"string"}}}'
        WordSearchResults: |-
                {"simple":false,"type":"object","properties":{"totalResults":{"format":"int32","required":false,"type":"integer"},"searchResults":{"required":false,"type":"array","items":{"required":false,"type":"ref","simpleRef":"WordSearchResult","refFormat":"INTERNAL","$ref":"#\/definitions\/WordSearchResult"}}}}
        AudioFile: '{"simple":false,"required":["id"],"type":"object","properties":{"voteAverage":{"format":"float","required":false,"type":"number"},"audioType":{"required":false,"type":"string"},"attributionText":{"required":false,"type":"string"},"description":{"required":false,"type":"string"},"voteWeightedAverage":{"format":"float","required":false,"type":"number"},"commentCount":{"format":"int32","required":false,"type":"integer"},"duration":{"format":"double","required":false,"type":"number"},"createdAt":{"format":"date","required":false,"type":"string"},"createdBy":{"required":false,"type":"string"},"attributionUrl":{"required":false,"type":"string"},"fileUrl":{"required":false,"type":"string"},"id":{"format":"int64","required":false,"type":"integer"},"voteCount":{"format":"int32","required":false,"type":"integer"},"word":{"required":false,"type":"string"}}}'
        Related: '{"simple":false,"type":"object","properties":{"label4":{"required":false,"type":"string"},"relationshipType":{"required":false,"type":"string"},"gram":{"required":false,"type":"string"},"words":{"required":false,"type":"array","items":{"required":false,"type":"string"}},"label1":{"required":false,"type":"string"},"label2":{"required":false,"type":"string"},"label3":{"required":false,"type":"string"}}}'
        WordObject: '{"simple":false,"required":["id"],"type":"object","properties":{"vulgar":{"required":false,"type":"string"},"canonicalForm":{"required":false,"type":"string"},"suggestions":{"required":false,"type":"array","items":{"required":false,"type":"string"}},"id":{"format":"int64","required":false,"type":"integer"},"originalWord":{"required":false,"type":"string"},"word":{"required":false,"type":"string"}}}'
        DefinitionSearchResults: |-
                {"simple":false,"type":"object","properties":{"totalResults":{"format":"int32","required":false,"type":"integer"},"results":{"required":false,"type":"array","items":{"required":false,"type":"ref","simpleRef":"Definition","refFormat":"INTERNAL","$ref":"#\/definitions\/Definition"}}}}
        Note: '{"simple":false,"type":"object","properties":{"noteType":{"required":false,"type":"string"},"pos":{"format":"int32","required":false,"type":"integer"},"appliesTo":{"required":false,"type":"array","items":{"required":false,"type":"string"}},"value":{"required":false,"type":"string"}}}'
        WordSearchResult: '{"simple":false,"type":"object","properties":{"count":{"format":"int64","required":false,"type":"integer"},"lexicality":{"format":"double","required":false,"type":"number"},"word":{"required":false,"type":"string"}}}'
        AudioType: '{"simple":false,"type":"object","properties":{"name":{"required":false,"type":"string"},"id":{"format":"int32","required":false,"type":"integer"}}}'
        Frequency: '{"simple":false,"type":"object","properties":{"year":{"format":"int32","required":false,"type":"integer"},"count":{"format":"int64","required":false,"type":"integer"}}}'
        ExampleSearchResults: |-
                {"simple":false,"type":"object","properties":{"examples":{"required":false,"type":"array","items":{"required":false,"type":"ref","simpleRef":"Example","refFormat":"INTERNAL","$ref":"#\/definitions\/Example"}},"facets":{"required":false,"type":"array","items":{"required":false,"type":"ref","simpleRef":"Facet","refFormat":"INTERNAL","$ref":"#\/definitions\/Facet"}}}}
securitySchemes: 
    - 
        apiKey: 
            type: x-apiKey
            describedBy: 
                headers: 
                    Authorization: 
                        type: string
                        required: false
                        repeat: false
securedBy: 
    - 
        apiKey
/account.json/user: 
    get: 
        description: Requires a valid auth_token to be set.
        headers: 
            auth_token: 
                description: The auth token of the logged-in user, obtained by calling /account.{format}/authenticate/{username} (described above)
                type: string
                required: true
                repeat: false
        responses: 
            "200": 
                description: No response was specified
/words.json/search/{query}: 
    get: 
        queryParameters: 
            excludePartOfSpeech: 
                description: Exclude these comma-delimited parts of speech
                type: string
                required: false
                repeat: false
            minDictionaryCount: 
                description: Minimum number of dictionary entries for words returned
                type: integer
                required: false
                repeat: false
                default: "1"
            caseSensitive: 
                description: Search case sensitive
                type: string
                required: false
                repeat: false
                default: "true"
            maxCorpusCount: 
                description: Maximum corpus frequency for terms
                type: integer
                required: false
                repeat: false
                default: "-1"
            minLength: 
                description: Minimum word length
                type: integer
                required: false
                repeat: false
                default: "1"
            limit: 
                description: Maximum number of results to return
                type: integer
                required: false
                repeat: false
                default: "10"
            minCorpusCount: 
                description: Minimum corpus frequency for terms
                type: integer
                required: false
                repeat: false
                default: "5"
            includePartOfSpeech: 
                description: Only include these comma-delimited parts of speech
                type: string
                required: false
                repeat: false
            skip: 
                description: Results to skip
                type: integer
                required: false
                repeat: false
                default: "0"
            maxDictionaryCount: 
                description: Maximum dictionary definition count
                type: integer
                required: false
                repeat: false
                default: "-1"
            maxLength: 
                description: Maximum word length
                type: integer
                required: false
                repeat: false
                default: "-1"
        responses: 
            "200": 
                description: No response was specified
        baseUriParameters: 
            query: 
                description: Search query
                type: string
                required: true
                repeat: false
/words.json/wordOfTheDay: 
    get: 
        queryParameters: 
            date: 
                description: Fetches by date in yyyy-MM-dd
                type: string
                required: false
                repeat: false
        responses: 
            "200": 
                description: No response was specified
